{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "parser": "@babel/eslint-parser",
        "prettier/prettier": "error",
        "no-console": "off",
        "plugins": ["prettier"]
    }
}
